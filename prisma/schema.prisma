datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model DiscordUser {
  id        Int      @id @default(autoincrement())
  balance   Int      @default(0)
  discordId String   @unique
  username  String
  joinedAt  DateTime @default(now())
}
